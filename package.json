{
  "name": "seneca-basic",
  "version": "0.5.1",
  "description": "Seneca utility plugin.",
  "main": "basic.js",
  "scripts": {
    "test": "lab -v -P test -L -t 80",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -L -t 80 -r html > docs/coverage.html",
    "annotate": "docco basic.js lib/*.js -o docs/annotated",
    "prettier": "prettier --write --no-semi --single-quote basic.js test/*.js lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-basic"
  },
  "keywords": [
    "seneca",
    "plugin",
    "util",
    "utility",
    "basic"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "contributors": [
    "Richard Rodger (http://richardrodger.com)",
    "Bogdan Rotund"
  ],
  "license": "MIT",
  "dependencies": {
    "async": "2.0.x",
    "lodash": "4.17.4",
    "nid": "0.3.2"
  },
  "devDependencies": {
    "async": "2.0.x",
    "coveralls": "2.11.x",
    "docco": "0.7.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-standard": "2.x.x",
    "lab": "13.1.x",
    "pre-commit": "1.1.x",
    "prettier": "^1.9.2",
    "seneca": "plugin",
    "seneca-entity": "1.2.x"
  },
  "files": [
    "LICENSE",
    "README.md",
    "basic.js",
    "lib/common.js",
    "lib/entity.js",
    "lib/note.js",
    "lib/uniqueid.js"
  ]
}
